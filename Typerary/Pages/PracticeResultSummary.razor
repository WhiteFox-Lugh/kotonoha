<RadzenText TextStyle="TextStyle.DisplayH3" TagName="TagName.H3" class="mb-3 cormonant">Result</RadzenText>
<RadzenText TextStyle="TextStyle.DisplayH5" TagName="TagName.H5" class="cormonant">Collect @(collectCount)</RadzenText>
<RadzenText TextStyle="TextStyle.DisplayH5" TagName="TagName.H5" class="cormonant">Wrong @(wrongCount)</RadzenText>
    <RadzenText TextStyle="TextStyle.DisplayH5" TagName="TagName.H5" class="cormonant">Accuracy @(accuracy.ToString("0.00")) %</RadzenText>

@code {
    private bool showValue = true;
    private int collectCount;
    private int wrongCount;
    private double accuracy;
    GaugeTickPosition tickPosition = GaugeTickPosition.Outside;

    [Parameter]
    public PracticeResult CurrentResult { get; set; }

    protected override async Task OnInitializedAsync(){
        collectCount = CurrentResult.GetCollectCount();
        wrongCount = CurrentResult.GetWrongCount();
        accuracy = CurrentResult.GetAccuracy();

    }
}